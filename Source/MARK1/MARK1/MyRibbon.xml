    <tabs>
      <tab idMso="TabAddIns">
        <group id="MFAGroup" label="MFA">
          <button id="VistorsRoom" label="Visitor's Room"
             screentip="Fifth Floor Visitor's Room" onAction="OnVisitorsRoomButton"
             size="large" imageMso="MicrosoftPowerPoint"
                  />
          <button id="TanglinRoom" label="Tanglin Room"
             screentip="Sixth Floor Tanglin Room" onAction="OnTanglinRoomButton"
             size="large" imageMso="MicrosoftPowerPoint"
                  />
        </group>
        
        <group id="IstanaGroup" label="Istana">
          <button id="PMOffice" label="PM's Office"
             onAction="OnTanglinRoomButton"
